{
  "plugins": [
    "add-module-exports",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-proposal-class-properties",
    "syntax-trailing-function-commas"
  ],
  "presets": [
    ["@babel/preset-env", { "modules": "commonjs"}],
    "@babel/preset-react",
    "@babel/preset-flow"
  ]
}
